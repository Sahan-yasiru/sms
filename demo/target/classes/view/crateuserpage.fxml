      </ImageView>
      <Label layoutX="244.0" layoutY="14.0" prefHeight="72.0" prefWidth="201.0" styleClass="box" stylesheets="@css/Dashborad.css" text="Register" />
      <VBox layoutX="41.0" layoutY="128.0" prefHeight="148.0" prefWidth="612.0" spacing="20.0">
         <children>
            <HBox prefHeight="33.0" prefWidth="612.0" spacing="50.0">
               <children>
                  <Label prefHeight="18.0" prefWidth="149.0" text="Admin_ID">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="labelAdminID" prefHeight="25.0" prefWidth="358.0" textFill="#f80000">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox prefHeight="33.0" prefWidth="612.0" spacing="50.0">
