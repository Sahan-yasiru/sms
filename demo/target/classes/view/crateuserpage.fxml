<AnchorPane prefHeight="482.0" prefWidth="688.0" styleClass="pane" stylesheets="@css/Dashborad.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.main.demo.controller.CrateUserController">
   <children>
      <ImageView fitHeight="482.0" fitWidth="688.0" rotate="180.0">
